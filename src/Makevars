PKG_LIBS = $(BLAS_LIBS) $(FLIBS)

FMODSRC = AOptim.f90
FSRC = calculusTransfo.f90 Jointhet.f90 hetmixCont.f90 hetmixContMult.f90 hetmixlin.f90 hetmixOrd.f90 predictCont.f90 predictMult.f90 postprob2.f90 cvpl.f90

# Define order of compilation
OBJECTS = $(FMODSRC:.f90=.o) $(FSRC:.f90=.o)

$(SHLIB): $(OBJECTS)


#Setup dependencies for parallel make
hetmixCont.o: AOptim.o
hetmixContMult.o: AOptim.o
hetmixlin.o: AOptim.o
hetmixOrd.o: AOptim.o
Jointhet.o: AOptim.o
predictCont.o: AOptim.o
predictMult.o: AOptim.o
postprob2.o:AOptim.o
cvpl.o: AOptim.o Jointhet.o


